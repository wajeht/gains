{
  "name": "gains",
  "version": "0.0.15",
  "description": "All in one tools to help strength athletes load the right amount of training dosage!",
  "main": "./src/app/server.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/app/server.js",
    "build:start": "npm run build && node ./src/app/server.js",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "kill:port": "./src/bin/kill-used-port.sh",
    "dev": "concurrently --kill-others --names \"EXPRESS,VUE\" -c \"bgGreen.black,bgRed.black\" \"nodemon ./src/app/server.js\" \"vite build --watch --mode development\"",
    "dev:ui": "vite",
    "dev:api": "nodemon ./src/app/server.js",
    "dev:hmr": "concurrently --kill-others --names \"EXPRESS,VUE\" -c \"bgGreen.black,bgRed.black\" \"HMR='true' nodemon ./src/app/server.js\" \"open-cli http://localhost:3000/ && vite\"",
    "build": "npm run build:ui && npm run build:api && npm run minify:html",
    "build:ui": "vite build",
    "build:api": "echo api does not need to build!",
    "migrate:make": "knex migrate:make --knexfile=./src/config/knexfile.js",
    "migrate:latest": "knex migrate:latest --knexfile=./src/config/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile=./src/config/knexfile.js",
    "seed:make": "knex seed:make --knexfile=./src/config/knexfile.js",
    "seed:run": "knex seed:run --knexfile=./src/config/knexfile.js",
    "reset:db": "npm run migrate:rollback && npm run migrate:latest && npm run seed:run",
    "release": "standard-version",
    "release:alpha": "npm run release -- --prerelease alpha",
    "rmds": "find . -name '.DS_Store' -delete",
    "prepare": "husky install",
    "commit": "git add -A && cz",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "lint": "eslint --fix .",
    "minify:html": "html-minifier --input-dir ./src --output-dir ./src --collapse-whitespace --minify-js --minify-css --remove-comments --file-ext html"
  },
  "bin": {
    "gains": "./src/app/cli/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wajeht/gains.git"
  },
  "keywords": [
    "gains",
    "workout",
    "powerlifting",
    "fitness",
    "bodybuilding",
    "squat",
    "bench",
    "deadlift",
    "press"
  ],
  "author": {
    "name": "wajeht",
    "url": "https://jaw.dev/"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/wajeht/gains/issues"
  },
  "homepage": "https://github.com/wajeht/gains#readme",
  "dependencies": {
    "@formkit/auto-animate": "^0.8.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@popperjs/core": "^2.11.8",
    "@sentry/node": "^7.88.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "adm-zip": "^0.5.10",
    "animate.css": "^4.1.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.2",
    "bull": "^4.11.5",
    "chalk": "^5.3.0",
    "chart.js": "^3.9.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-jsdoc-swagger": "^1.8.0",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.0.1",
    "filesize": "^10.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "html-minifier": "^4.0.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "knex-paginate": "^3.1.1",
    "lodash-es": "^4.17.21",
    "marked": "^8.0.1",
    "minimist": "^1.2.8",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3",
