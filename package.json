{
  "name": "gains",
  "version": "0.0.2",
  "description": "The best way to track your workouts",
  "main": "server.js",
  "type": "module",
  "engines": {
    "npm": ">=8.11.0",
    "node": ">=16.16.0"
  },
  "scripts": {
    "start": "vite build && node ./src/bin/server.js",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "dev": "concurrently --kill-others --names \"EXPRESS,VUE\" -c \"bgGreen.black,bgRed.black\" \"nodemon ./src/bin/server.js\" \"vite build --watch --mode development\"",
    "dev:ui": "vite",
    "dev:api": "nodemon ./src/bin/server.js",
    "dev:hmr": "concurrently --kill-others --names \"EXPRESS,VUE\" -c \"bgGreen.black,bgRed.black\" \"HMR='true' nodemon ./src/bin/server.js\" \"open-cli http://localhost:8081/ && vite\"",
    "build": "npm run build:ui && npm run build:api",
    "build:ui": "vite build",
    "build:api": "echo api does not need to build!",
    "migrate:make": "knex migrate:make --knexfile=./src/config/knexfile.js",
    "migrate:latest": "knex migrate:latest --knexfile=./src/config/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile=./src/config/knexfile.js",
    "seed:make": "knex seed:make --knexfile=./src/config/knexfile.js",
    "seed:run": "knex seed:run --knexfile=./src/config/knexfile.js",
    "reset:db": "npm run migrate:rollback && npm run migrate:latest && npm run seed:run",
    "release": "standard-version",
    "release:alpha": "npm run release -- --prerelease alpha",
    "rmds": "find . -name '.DS_Store' -delete",
    "prepare": "husky install",
    "commit": "git add -A && cz"
  },
  "bin": {
    "gains": "./src/bin/gains.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allkindsofgains/gains.git"
  },
  "keywords": [
    "gains",
    "workout",
    "powerlifting",
    "fitness",
    "bodybuilding",
    "squat",
    "bench",
    "deadlift",
    "press"
  ],
  "author": "wajeht",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/allkindsofgains/gains/issues"
  },
  "homepage": "https://github.com/allkindsofgains/gains#readme",
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@jambonn/vue-lazyload": "^1.0.9",
    "@popperjs/core": "^2.11.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "animate.css": "^4.1.1",
    "aos": "^3.0.0-beta.6",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.9.1",
    "cac": "^6.7.12",
    "chalk": "^5.0.1",
    "chart.js": "^3.9.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.3",
    "discord.js": "^13.8.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-jsdoc-swagger": "^1.7.0",
    "express-rate-limit": "^6.4.0",
    "express-validator": "^6.14.2",
    "filesize": "^9.0.11",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.2.0",
    "knex-paginate": "^3.0.1",
    "lodash-es": "^4.17.21",
    "marked": "^4.0.18",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.1",
    "nodemailer": "^6.7.6",
    "papaparse": "^5.3.2",
    "pg": "^8.7.3",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate": "^1.6.1",
    "pino": "^8.1.0",
    "pino-pretty": "^8.1.0",
    "request-ip": "^3.0.2",
    "shelljs": "^0.8.5",
    "supertest": "^6.2.3",
    "typed.js": "^2.0.12",
    "uuid": "^8.3.2",
    "vite": "^2.9.14",
    "vue": "^3.2.37",
    "vue-chart-3": "^3.1.8",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash-es": "^4.17.6",
    "@types/multer": "^1.4.7",
    "@types/request-ip": "^0.0.37",
    "c8": "^7.11.3",
    "commitizen": "^4.2.5",
    "concurrently": "^7.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.18",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sleep-ms": "^2.0.1",
    "standard-version": "^9.5.0",
    "vitest": "^0.15.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
