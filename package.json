    "request-ip": "^3.3.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "supertest": "^7.0.0",
    "useragent": "^2.3.0",
    "uuid": "^9.0.1",
    "vite": "^4.5.1",
