{
  "name": "gains",
  "version": "1.0.0",
  "description": "The best way to track your workouts",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "vite build && node ./src/bin/server.js",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "dev": "concurrently --kill-others --names \"EXPRESS,VUE\" -c \"bgGreen.black,bgRed.black\" \"nodemon ./src/bin/server.js\" \"vite build --watch --mode development\"",
    "dev:ui": "vite",
    "dev:api": "nodemon ./src/bin/server.js",
    "build": "npm run build:ui && npm run build:api",
    "build:ui": "vite build",
    "build:api": "echo does not need to build!",
    "migrate:make": "knex migrate:make --knexfile=./src/config/knexfile.js",
    "migrate:latest": "knex migrate:latest --knexfile=./src/config/knexfile.js",
    "migrate:rollback": "knex migrate:rollback --knexfile=./src/config/knexfile.js",
    "seed:make": "knex seed:make --knexfile=./src/config/knexfile.js",
    "seed:run": "knex seed:run --knexfile=./src/config/knexfile.js",
    "rmds": "find . -name '.DS_Store' -delete"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allkindsofgains/gains.git"
  },
  "keywords": [
    "gains",
    "workout",
    "powerlifting",
    "fitness",
    "bodybuilding",
    "squat",
    "bench",
    "deadlift",
    "press"
  ],
  "author": "wajeht",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/allkindsofgains/gains/issues"
  },
  "homepage": "https://github.com/allkindsofgains/gains#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@popperjs/core": "^2.11.5",
    "@vitejs/plugin-vue": "^2.3.3",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.3",
    "cac": "^6.7.12",
    "chalk": "^5.0.1",
    "chart.js": "^3.8.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "express-validator": "^6.14.2",
    "helmet": "^5.1.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.1.0",
    "lodash-es": "^4.17.21",
    "nodemailer": "^6.7.5",
    "pg": "^8.7.3",
    "pinia": "^2.0.14",
    "pino": "^8.1.0",
    "pino-pretty": "^8.1.0",
    "supertest": "^6.2.3",
    "vite": "^2.9.12",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "c8": "^7.11.3",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.18",
    "vitest": "^0.15.2"
  }
}
